<platform:DialogWindow  x:Class="EM2AExtension.InterfaceWizard"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:platform="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
                        xmlns:toolkit="clr-namespace:Community.VisualStudio.Toolkit;assembly=Community.VisualStudio.Toolkit"
                        toolkit:Themes.UseVsTheme="True"
                        mc:Ignorable="d"
                        Width="405"
                        Height="117"
                        ResizeMode="NoResize">
    <Grid>
        <Button Command="{Binding CmdAddInterface}" Content="Add" HorizontalAlignment="Left" Height="34" Margin="300,0,0,0" VerticalAlignment="Center" Width="76"/>
        <TextBox Text="{Binding InterfaceName}" HorizontalAlignment="Left" Height="35" Margin="80,0,0,0" VerticalAlignment="Center" Width="212"/>
        <Label Content="Interface" HorizontalAlignment="Left" Height="30" Margin="10,0,0,0" VerticalAlignment="Center" Width="108"/>

    </Grid>
</platform:DialogWindow>
