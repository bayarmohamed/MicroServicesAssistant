<platform:DialogWindow  x:Class="EM2AExtension.Wizard"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:platform="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
                        xmlns:toolkit="clr-namespace:Community.VisualStudio.Toolkit;assembly=Community.VisualStudio.Toolkit"
                        toolkit:Themes.UseVsTheme="True"
                        mc:Ignorable="d"
                        Width="600"
                        Height="400"
                        d:DesignHeight="600"
                        d:DesignWidth="400">
    <Grid>
        <ComboBox ItemsSource="{Binding Projects}" HorizontalAlignment="Left" Height="22" Margin="94,37,0,0" VerticalAlignment="Top" Width="127"/>
        <Label Content="Project" HorizontalAlignment="Left" Height="34" Margin="17,37,0,0" VerticalAlignment="Top" Width="55"/>
        <Button Command="{Binding CmdAddFile}" Content="Add file" HorizontalAlignment="Left" Height="28" Margin="300,37,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</platform:DialogWindow>
